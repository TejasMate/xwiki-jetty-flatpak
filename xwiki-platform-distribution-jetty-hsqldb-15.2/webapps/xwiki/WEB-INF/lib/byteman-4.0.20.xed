        <connection>
            <system>git</system>
            <path>git://github.com/adinn/byteman.git/byteman</path>
        </connection>
        <developerconnection>
            <system>git</system>
            <path>git://github.com/adinn/byteman.git/byteman</path>
        </developerconnection>
    </scm>
    <issuemanagement>
