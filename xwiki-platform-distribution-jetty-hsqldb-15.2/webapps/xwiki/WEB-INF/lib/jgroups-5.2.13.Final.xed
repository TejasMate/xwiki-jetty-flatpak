<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extension>
    <id>org.jgroups:jgroups</id>
    <version>5.2.13.Final</version>
    <type>jar</type>
    <name>JGroups</name>
    <summary>Reliable cluster communication toolkit</summary>
    <website>http://www.jgroups.org</website>
    <authors>
        <author>
            <name>Bela Ban</name>
        </author>
    </authors>
    <licenses>
        <license>
            <name>Apache License 2.0</name>
        </license>
    </licenses>
    <scm>
        <url>https://github.com/belaban/JGroups</url>
        <connection>
            <system>git</system>
            <path>git://github.com/belaban/JGroups.git</path>
        </connection>
        <developerconnection>
            <system>git</system>
            <path>ssh://git@github.com/belaban/JGroups.git</path>
        </developerconnection>
    </scm>
    <issuemanagement>
        <system>Jira</system>
        <url>https://issues.redhat.com/browse/JGRP</url>
    </issuemanagement>
    <dependencies>
        <dependency>
            <id>org.jboss.byteman:byteman-bmunit</id>
            <version>4.0.20</version>
            <optional type="boolean">false</optional>
            <properties>
                <maven.dependency.scope>compile</maven.dependency.scope>
                <maven.dependency.type>jar</maven.dependency.type>
            </properties>
        </dependency>
    </dependencies>
    <repositories>
        <repository>
            <id>xwiki-snapshots</id>
            <type>maven</type>
        </repository>
        <repository>
            <id>xwiki-releases</id>
            <type>maven</type>
        </repository>
        <repository>
            <id>jboss-public-repository-group</id>
            <type>maven</type>
        </repository>
        <repository>
            <id>jboss-snapshots-repository</id>
            <type>maven</type>
        </repository>
    </repositories>
    <properties>
        <maven.groupid>org.jgroups</maven.groupid>
        <maven.artifactid>jgroups</maven.artifactid>
    </properties>
</extension>
