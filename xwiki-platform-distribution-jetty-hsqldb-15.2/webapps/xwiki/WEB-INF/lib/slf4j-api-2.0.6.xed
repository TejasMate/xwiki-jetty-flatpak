<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extension>
    <id>org.slf4j:slf4j-api</id>
    <version>2.0.6</version>
    <type>jar</type>
    <name>SLF4J API Module</name>
    <summary>The slf4j API</summary>
    <website>http://www.slf4j.org</website>
    <authors>
        <author>
            <name>Ceki Gulcu</name>
        </author>
    </authors>
    <licenses>
        <license>
            <name>The MIT License</name>
        </license>
    </licenses>
    <scm>
        <url>https://github.com/qos-ch/slf4j/slf4j-api</url>
        <connection>
            <system>git</system>
            <path>https://github.com/qos-ch/slf4j.git/slf4j-api</path>
        </connection>
    </scm>
    <manageddependencies>
        <dependency>
            <id>org.slf4j:slf4j-api</id>
            <version>2.0.6</version>
            <optional type="boolean">false</optional>
            <properties>
                <maven.dependency.type>jar</maven.dependency.type>
            </properties>
        </dependency>
        <dependency>
            <id>org.slf4j:slf4j-jdk14</id>
            <version>2.0.6</version>
            <optional type="boolean">false</optional>
            <properties>
                <maven.dependency.type>jar</maven.dependency.type>
            </properties>
        </dependency>
        <dependency>
            <id>org.slf4j:slf4j-simple</id>
            <version>2.0.6</version>
            <optional type="boolean">false</optional>
            <properties>
                <maven.dependency.type>jar</maven.dependency.type>
            </properties>
        </dependency>
        <dependency>
            <id>ch.qos.reload4j:reload4j</id>
            <version>1.2.22</version>
            <optional type="boolean">false</optional>
            <properties>
                <maven.dependency.type>jar</maven.dependency.type>
            </properties>
        </dependency>
        <dependency>
            <id>ch.qos.cal10n:cal10n-api</id>
            <version>0.8.1</version>
            <optional type="boolean">false</optional>
            <properties>
                <maven.dependency.type>jar</maven.dependency.type>
            </properties>
        </dependency>
    </manageddependencies>
    <repositories>
        <repository>
            <id>xwiki-snapshots</id>
            <type>maven</type>
        </repository>
        <repository>
            <id>xwiki-releases</id>
            <type>maven</type>
        </repository>
    </repositories>
    <properties>
        <maven.groupid>org.slf4j</maven.groupid>
        <maven.artifactid>slf4j-api</maven.artifactid>
    </properties>
</extension>
